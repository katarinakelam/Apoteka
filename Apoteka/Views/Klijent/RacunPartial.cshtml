@model List<Apoteka.ViewModels.RacunVM>

<table class="table">
    <tr>
        <th>
            Redni broj računa
        </th>
        <th>
            Datum i vrijeme izdavanja
        </th>
        <th>
            Korisnik
        </th>
        <th>
           Klijent
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RacunId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DatumIvrijemeIzdavanja)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KorisnikNaziv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KlijentNaziv)
            </td>
        </tr>
    }

</table>
